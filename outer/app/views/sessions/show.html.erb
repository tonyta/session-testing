<div>
  <h3>Cookies:</h3>
  <ul>
  <% cookies.each do |key, _| %>
    <li><%= key %>
  <% end %>
  </ul>
</div>

<div>msg: <%= msg.inspect %></div>
<%= button_to "set new msg", new_sessions_path, method: "get" %>
